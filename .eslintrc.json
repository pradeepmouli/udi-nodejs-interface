{
  "extends": "strongloop",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "rules": {
    "indent": [
      "warn",
      2, {
        "MemberExpression": 0,
        "SwitchCase": 1,
        "CallExpression": {
          "arguments": "off"
        }
      }
    ]}
}
